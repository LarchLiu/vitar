    "lint": "eslint . --ext=.ts,.vue",
    "prepublishOnly": "npm run build",
    "release": "zx scripts/release.mjs",
    "test": "vitest"
  },
  "style": "./dist/index.css",
  "dependencies": {
    "@mediapipe/camera_utils": "^0.3.1632432234",
