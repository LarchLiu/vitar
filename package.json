    "pixi-live2d-display": "^0.3.1",
    "pixi.js": "^6.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.1",
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^2.3.3",
