  "repository": {
    "type": "git",
    "url": "git+https://github.com/LarchLiu/vitar.git"
  },
  "main": "dist/vitar.cjs.js",
  "module": "dist/vitar.es.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "ci:publish": "zx scripts/publish.mjs",
